<%- include ("../partials/head.ejs") %>
<title>APOD Album Maker</title>
</head>

<body>
  <form action="" method="POST">
    <input type="text" name="header">
    <input type="text" name="title" value="<%= results.title %>">
    <input type="text" name="date" value="<%= date %>" readonly>
    <img src="<%= hdurl %>" title="<%= title %>">
    <input type="text" name="copyright" value="<%= copyright %>" readonly>
    <input type="hidden" name="hdurl" value="<%= hdurl %>">
  </form>
</body>
</html>